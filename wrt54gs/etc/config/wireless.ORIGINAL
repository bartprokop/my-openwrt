config wifi-device  radio0
	option type     mac80211
	option channel  11
	option hwmode	11g
	option path	'ssb0:0'
	option htmode	
	# REMOVE THIS LINE TO ENABLE WIFI:
	option disabled 1

config wifi-iface
	option device   radio0
	option network  lan
	option mode     ap
	option ssid     OpenWrt
	option encryption none

